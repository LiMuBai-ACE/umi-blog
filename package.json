{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@types/node": "^14.0.27",
    "@umijs/plugin-esbuild": "^1.0.0-beta.2",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/preset-react": "^1.5.19",
    "@umijs/test": "^3.2.14",
    "add": "^2.0.6",
    "antd": "^4.5.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "braft-editor": "^2.3.9",
    "braft-extensions": "^0.1.0",
    "compression-webpack-plugin": "^4.0.0",
    "crypto-js": "^4.0.0",
    "draft-js-prism": "^1.0.6",
    "dva": "^2.4.1",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.0.5",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-cookies": "^0.1.1",
    "react-dom": "^16.13.1",
    "sass-loader": "^9.0.2",
    "umi": "^3.2.14",
    "yarn": "^1.22.4",
    "yorkie": "^2.0.0"
  }
}
