{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/plugin-esbuild": "^1.0.0-beta.2",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/preset-react": "^1.5.19",
    "@umijs/test": "^3.2.10",
    "add": "^2.0.6",
    "antd": "^4.4.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "compression-webpack-plugin": "^4.0.0",
    "dva": "^2.4.1",
    "lint-staged": "^10.0.7",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^9.0.2",
    "umi": "^3.2.10",
    "yarn": "^1.22.4",
    "yorkie": "^2.0.0"
  }
}
